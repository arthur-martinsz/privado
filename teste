#include <stdio.h>

#define MAX_CITIES 6

// Define the distance matrix with -1 indicating no access.
int distance[MAX_CITIES][MAX_CITIES] = {
    {0, 63, 210, 190, -1, 190},
    {63, 0, 160, 150, 95, 10},
    {210, 160, 0, 10, 1, 10},
    {190, 150, 10, 0, 10, 20},
    {10, 95, 7, 21, 0, 80},
    {190, 2, -1, 41, 80, 0}
};

int main() {
    int N;
    
    // Read the number of cities in the route
    scanf("%d", &N);
    if (N <= 0 || N > 100) {
        printf("rota invalida!\n");
        return 1;
    }
    
    int route[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &route[i]);
    }
    
    int total_distance = 0;
    int valid = 1;
    
    // Calculate the total distance of the route
    for (int i = 0; i < N - 1; i++) {
        int from = route[i];
        int to = route[i + 1];
        
        if (distance[from][to] == -1) {
            valid = 0;
            break;
        } else {
            total_distance += distance[from][to];
        }
    }
    
    if (valid) {
        printf("%d\n", total_distance);
    } else {
        printf("rota invalida!\n");
    }
    
    return 0;
}
